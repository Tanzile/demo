
page {
	background: #ccc;
}
.box {
	width: 100%;
	height: 100%;
}
.bt {
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 47px;
	line-height: 47px;
	-webkit-box-pack: space-evenly;
	-webkit-justify-content: space-evenly;
	        justify-content: space-evenly;
	background: #fff;
}
.list {
	position: relative;
	margin: 0 auto;
	margin-top: 10px;
	width: 355px;
	height: 50px;
	border: 1px solid #ccc;
	background: #fff;
	border-radius: 8px 8px 0 0;
}
.list_left {
	position: absolute;
	width: 100%;
	height: 50px;
	line-height: 50px;
	border-bottom: 1px solid #ccc;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.list_left>image:nth-child(1) {
	width: 20px;
	height: 20px;
	margin-top: 15px;
	margin-left: 10px;
	margin-right: 5px;
}
.list_left>text {
	font-size: 13px;
}
.list_left>image:nth-child(3) {
	width: 20px;
	height: 25px;
	margin-top: 12px;
	margin-left: 3px;
}
.list_r {
	position: absolute;
	right: 20px;
}
.list_r>image {
	width: 60px;
	height: 50px;
}
.active {
	border-bottom: 4px solid #F0AD4E;
	border-radius: 1px;
	color: #F0AD4E;
}
.list_box {
	position: relative;
	margin: 0 auto;
	width: 355px;
	height: 190px;
	border: 1px solid #ccc;
	background: #fff;
	border-radius: 0 0 8px 8px;
}
.img>image {
	width: 80px;
	height: 80px;
}
